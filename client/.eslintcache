me,\n      materialsNeeded,\n      userId,\n      errors,\n    } = this.state;\n\n    if (this.state.ownerId === authUser.id) {\n      return (\n        <div id=\"root\">\n          <div className=\"bounds course--detail\">\n            <h1>Update Course No:{this.state.results.id}</h1>\n            <Form\n              cancel={this.cancel}\n              errors={errors}\n              submit={this.submit}\n              submitButtonText=\"Update Course\"\n              elements={() => (\n                <React.Fragment>\n                  <div className=\"grid-66\">\n                    <div className=\"course--header\">\n                      <h4 className=\"course--label\">Course</h4>\n                      <div>\n                        <input\n                          id=\"title\"\n                          name=\"title\"\n                          type=\"text\"\n                          className=\"input-title course--title--input\"\n                          defaultValue={this.state.title}\n                          onChange={this.change}\n                        />\n                      </div>\n                    </div>\n                    <div>\n                      <p>\n                        By {this.state.ownerFirstName}{\" \"}\n                        {this.state.ownerLastName}\n                      </p>\n                    </div>\n                    <div className=\"course--description\">\n                      <div>\n                        <textarea\n                          id=\"description\"\n                          name=\"description\"\n                          className\n                          defaultValue={this.state.description}\n                          onChange={this.change}\n                        />\n                      </div>\n                    </div>\n                  </div>\n                  <div className=\"grid-25 grid-right\">\n                    <div className=\"course--stats\">\n                      <ul className=\"course--stats--list\">\n                        <li className=\"course--stats--list--item\">\n                          <h4>Estimated Time</h4>\n                          <div>\n                            <input\n                              id=\"estimatedTime\"\n                              name=\"estimatedTime\"\n                              type=\"text\"\n                              className=\"course--time--input\"\n                              defaultValue={this.state.estimatedTime}\n                              onChange={this.change}\n                            />\n                          </div>\n                        </li>\n                        <li className=\"course--stats--list--item\">\n                          <h4>Materials Needed</h4>\n                          <div>\n                            <textarea\n                              id=\"materialsNeeded\"\n                              name=\"materialsNeeded\"\n                              className\n                              defaultValue={this.state.materialsNeeded}\n                              onChange={this.change}\n                            />\n                          </div>\n                        </li>\n                      </ul>\n                    </div>\n                  </div>\n                </React.Fragment>\n              )}\n            />\n          </div>\n        </div>\n      );\n    } else {\n      return (\n        <div id=\"root\">\n          <div className=\"bounds course--detail\">\n            <h1>\n              Unfortunately, you are not authorised to update this record!\n            </h1>\n          </div>\n        </div>\n      );\n    }\n  }\n\n  change = (event) => {\n    const name = event.target.name;\n    const value = event.target.value;\n\n    this.setState(() => {\n      return {\n        [name]: value,\n      };\n    });\n  };\n\n  submit = () => {\n    const { context } = this.props;\n    const authUser = context.authenticatedUser;\n    const emailAddress = authUser.emailAddress;\n    const password = authUser.password;\n\n    const { title, description, estimatedTime, materialsNeeded } = this.state;\n\n    const course = {\n      title: title,\n      description: description,\n      estimatedTime: estimatedTime,\n      materialsNeeded: materialsNeeded,\n    };\n\n    const courseId = this.props.match.params.id;\n\n    context.data\n      .updateCourse(course, courseId, emailAddress, password)\n      .then((errors) => {\n        if (errors.length) {\n          this.setState({ errors });\n        } else {\n          console.log(`${title} has been successfully updated!!`);\n          alert(\"Course successfully updated!\");\n          this.props.history.push(\"/courses\");\n        }\n      })\n      .then(console.log(\"Hmm\" + course.title))\n      .catch((err) => {\n        console.log(err);\n        this.props.history.push(\"/courses\");\n      });\n  };\n}\n\nexport default UpdateCourse;\n","/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App/client/src/Context.js",[],"/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App/client/src/Data.js",[],"/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App/client/src/components/Form.js",[],"/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App/client/src/config.js",["90"],"export default {\n  apiBaseUrl: \"http://localhost:5000/api\",\n};\n","/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App/client/src/components/Authenticated.js",["91"],"import React from \"react\";\n\nexport default ({ context }) => {\n  const authUser = context.authenticatedUser;\n\n  return (\n    <div id=\"root\">\n      <div className=\"bounds\">\n        <div className=\"grid-100\">\n          <h1>\n            {authUser.firstName} {authUser.lastName} is authenticated!\n          </h1>\n          <p>Your username is {authUser.emailAddress}</p>\n  