 <div className=\"grid-100\">\n                  <span>\n                    {this.owner ? (\n                      <React.Fragment>\n                        <Link\n                          className=\"button\"\n                          to={`/courses/${this.state.results.id}/update`}\n                        >\n                          Update Course\n                        </Link>\n                        <Link className=\"button\" onClick={this.delete}>\n                          Delete Course\n                        </Link>\n                      </React.Fragment>\n                    ) : null}\n                  </span>\n                  {!this.owner && authUser ? (\n                    <Link className=\"button\" to=\"/courses/create\">\n                      Create a Course\n                    </Link>\n                  ) : null}\n                  {authUser ? null : (\n                    <Link className=\"button\" to=\"/sign-up\">\n                      Sign Up\n                    </Link>\n                  )}\n                  <Link className=\"button button-secondary\" to=\"/courses\">\n                    Return to List\n                  </Link>\n                </div>\n              </div>\n            </div>\n            <div className=\"bounds course--detail\">\n              <div className=\"grid-66\">\n                <div className=\"course--header\">\n                  <h4 className=\"course--label\">Course</h4>\n                  <h3 className=\"course--title\">{this.state.results.title}</h3>\n                  <p>\n                    By {this.state.ownerFirstName} {this.state.ownerLastName}\n                  </p>\n                </div>\n                <div className=\"course--description\">\n                  {this.state.desc.map((para) => (\n                    <p>{para}</p>\n                  ))}\n                </div>\n              </div>\n              <div className=\"grid-25 grid-right\">\n                <div className=\"course--stats\">\n                  <ul className=\"course--stats--list\">\n                    <li className=\"course--stats--list--item\">\n                      <h4>Estimated Time</h4>\n                      <h3>{this.state.results.estimatedTime}</h3>\n                    </li>\n                    <li className=\"course--stats--list--item\">\n                      <h4>Materials Needed</h4>\n                      <ul>\n                        {this.state.materials.map((item) => (\n                          <li>{item}</li>\n                        ))}\n                      </ul>\n                    </li>\n                  </ul>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n\n  delete = () => {\n    const { context } = this.props;\n    const authUser = context.authenticatedUser;\n    const emailAddress = authUser.emailAddress;\n    const password = authUser.password;\n\n    const { title, description, estimatedTime, materialsNeeded } = this.state;\n\n    const course = {\n      title,\n      description,\n      estimatedTime,\n      materialsNeeded,\n    };\n\n    const cId = this.props.match.params.id;\n\n    console.log(cId);\n\n    context.data\n      .deleteCourse(course, cId, emailAddress, password)\n      .then((errors) => {\n        if (errors.length) {\n          this.setState({ errors });\n        } else {\n          console.log(\"Course deleted\");\n          this.props.history.push(\"/courses\");\n        }\n      })\n      .catch((err) => {\n        console.log(err);\n        this.props.history.push(\"/courses\");\n      });\n  };\n}\n\nexport default CourseDetail;\n","/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App/client/src/components/Header.js",[],"/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App/client/src/components/CourseButton.js",[],"/Users/JonnyB/Desktop/MASTER/CODE/JS/Projects/JS-TD-P10-Full-Stack-React-App